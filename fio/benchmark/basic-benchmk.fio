[global]
bs=4k
direct=1
iodepth=1
runtime=60s
size=100m
time_based
numjobs=1
rwmixread=50

# TEST ONE: normal read-write

[basic_readwrite]
filename=/data/apt/fio.rw.test1
ioengine=sync
rw=readwrite

# TEST TWO: random read-write

[rand_readwrite]
filename=/data/apt/fio.rw.test2
ioengine=sync
rw=randrw
